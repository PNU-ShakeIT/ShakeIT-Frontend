<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="start"
    android:id="@+id/drawer"
    >


    <View
        android:id="@+id/view7"
        android:layout_width="456dp"
        android:layout_height="270dp"
        android:background="@drawable/congress_main_search"
        android:elevation="5dp"
        android:paddingBottom="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.566" />

    <View
        android:id="@+id/view8"
        android:layout_width="461dp"
        android:layout_height="274dp"
        android:background="@drawable/congress_main_search"
        android:elevation="5dp"
        android:paddingBottom="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.556"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.897" />

    <View
        android:id="@+id/view6"
        android:layout_width="480dp"
        android:layout_height="2dp"
        android:background="#F4F4F4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.148" />

    <View
        android:id="@+id/view5"
        android:layout_width="250dp"
        android:layout_height="60dp"
        android:background="@drawable/congress_symbol_blue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.081" />

    <GridLayout
        android:layout_width="453dp"
        android:layout_height="292dp"
        android:background="@drawable/congress_main_search"
        android:columnCount="4"
        android:elevation="5dp"
        android:padding="12dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.228">

        <FrameLayout
            android:layout_width="90dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:elevation="6dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.167"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.217">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:layout_gravity="bottom"
                android:gravity="center"
                android:paddingStart="1dp"
                android:text="일정"
                android:textColor="@color/black"
                android:textSize="30dp" />

            <View
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:background="@drawable/calendar"
                android:elevation="6dp"
                tools:layout_editor_absoluteX="78dp"
                tools:layout_editor_absoluteY="240dp" />
        </FrameLayout>


        <FrameLayout
            android:layout_width="111dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:elevation="6dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.167"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.217">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="37dp"
                android:layout_gravity="bottom"
                android:text="국회의원"
                android:textColor="@color/black"
                android:textSize="30dp" />

            <View
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/user"
                android:elevation="6dp"
                tools:layout_editor_absoluteX="78dp"
                tools:layout_editor_absoluteY="240dp" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="90dp"
            android:layout_height="120dp"
            android:layout_marginRight="10dp"
            android:layout_margin="5dp"
            android:elevation="6dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.167"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.217">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:layout_gravity="bottom"
                android:gravity="center"
                android:paddingStart="1dp"
                android:text="뉴스"
                android:textColor="@color/black"
                android:textSize="30dp" />

            <View
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:background="@drawable/news"
                android:elevation="6dp"
                tools:layout_editor_absoluteX="78dp"
                tools:layout_editor_absoluteY="240dp" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="90dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:elevation="6dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.167"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.217">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:layout_gravity="bottom"
                android:gravity="center"
                android:paddingStart="1dp"
                android:text="도서관"
                android:textColor="@color/black"
                android:textSize="30dp" />

            <View
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:background="@drawable/library"
                android:elevation="6dp"
                tools:layout_editor_absoluteX="78dp"
                tools:layout_editor_absoluteY="240dp" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="90dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:elevation="6dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.167"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.217">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:layout_gravity="bottom"
                android:gravity="center"
                android:paddingStart="1dp"
                android:text="입법"
                android:textColor="@color/black"
                android:textSize="30dp" />

            <View
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:background="@drawable/ip_bub"
                android:elevation="6dp"
                tools:layout_editor_absoluteX="78dp"
                tools:layout_editor_absoluteY="240dp" />
        </FrameLayout>

        <FrameLayout
            android:layout_width="90dp"
            android:layout_height="120dp"
            android:layout_margin="5dp"
            android:layout_marginLeft="15dp"
            android:elevation="6dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.167"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.217">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="37dp"
                android:layout_gravity="bottom"
                android:gravity="center"
                android:paddingStart="1dp"
                android:text="청원"
                android:textColor="@color/black"
                android:textSize="30dp" />

            <View
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:background="@drawable/chungwon"
                android:elevation="6dp"
                tools:layout_editor_absoluteX="78dp"
                tools:layout_editor_absoluteY="240dp" />
        </FrameLayout>


    </GridLayout>

</androidx.constraintlayout.widget.ConstraintLayout>